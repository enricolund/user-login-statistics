# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=user_login_statistics
DATABASE_URL=postgresql://postgres:postgres@database:5432/user_login_statistics?schema=public

# Server Configuration
API_PORT=37001
WS_PORT=37002
WS_PATH=/ws

# Application Configuration
NODE_ENV=production
CORS_ORIGINS=http://localhost:3000

# Stats and Caching Configuration
STATS_AGGREGATION_INTERVAL=30
CACHE_TTL_MINUTES=5

# Default Values Configuration
DEFAULT_FAKE_DATA_COUNT=10
DEFAULT_TOP_USERS_LIMIT=10
DEFAULT_LOGIN_TRENDS_DAYS=30

# Rate Limiting Configuration
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=60000
