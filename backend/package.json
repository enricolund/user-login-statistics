{
  "private": true,
  "name": "@ORGANIZATION/PROJECT",
  "version": "0.1.0",
  "description": "Starter kit of Nestia",
  "main": "lib/index.js",
  "scripts": {
    "benchmark": "node bin/test/benchmark",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest-e2e.config.json",
    "test:e2e:watch": "jest --config jest-e2e.config.json --watch",
    "test:webpack": "npm run webpack && node bin/test/webpack.js",
    "------------------------BUILDS------------------------": "",
    "build": "npm run build:sdk && npm run build:main && npm run build:test",
    "build:api": "rimraf packages/api/lib && nestia all && rimraf packages/api/lib && tsc -p packages/api/tsconfig.json && rollup -c packages/api/rollup.config.js",
    "build:main": "rimraf lib && tsc",
    "build:sdk": "rimraf src/api/functional && nestia sdk",
    "build:test": "rimraf bin && tsc",
    "dev": "npm run build:test -- --watch",
    "eslint": "eslint src && eslint test",
    "eslint:fix": "eslint --fix src && eslint --fix test",
    "prepare": "ts-patch install",
    "prettier": "prettier src --write && prettier test --write",
    "start": "node lib/src/executable/server",
    "start:dev": "nest start --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samchon/nestia-start"
  },
  "keywords": [
    "nestia",
    "template",
    "boilerplate"
  ],
  "author": "AUTHOR",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samchon/nestia-start/issues"
  },
  "homepage": "https://github.com/samchon/nestia-start#readme",
  "devDependencies": {
    "@autobe/interface": "^0.10.6",
    "@faker-js/faker": "^9.9.0",
    "@nestia/benchmark": "^7.3.2",
    "@nestia/e2e": "^7.3.2",
    "@nestia/sdk": "^7.3.2",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/config": "^4.0.2",
    "@nestjs/testing": "^11.1.6",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/cli": "^0.11.21",
    "@types/cli-progress": "^3.11.5",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^8.2.5",
