    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/platform-ws": "^11.1.5",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/websockets": "^11.1.5",
    "@prisma/client": "^6.13.0",
    "cache-manager": "^7.1.1",
    "commander": "10.0.0",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
