    "cli": "^1.0.1",
    "cli-progress": "^3.12.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "express": "^4.18.2",
    "jest": "^30.0.5",
    "nestia": "^7.3.2",
    "prettier": "^3.2.4",
    "prettier-plugin-prisma": "^5.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^4.18.0",
